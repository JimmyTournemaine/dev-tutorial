{
  "name": "dev-tutorial-api",
  "version": "0.0.0",
  "private": true,
  "config": {
    "mocha": "mocha -r ts-node/register 'src/**/*.spec.ts' --require choma --trace-warnings"
  },
  "scripts": {
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.ts'",
    "serve": "nodemon -e ts --exec ts-node src/index.ts",
    "test": "nodemon -e ts --exec \"nyc $npm_package_config_mocha --exit\"",
    "test-ci": "nyc $npm_package_config_mocha --reporter xunit --reporter-option output=report/TESTS-ALL.xml --exit"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "default-exclude": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/index.ts",
      "**/*.spec.ts",
      "**/*.test.ts",
      "**/*.d.ts",
      "**/*.js"
    ]
  },
  "dependencies": {
    "async": "^3.2.0",
    "bash-parser": "^0.5.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dockerode": "^3.2.1",
    "express": "~4.17.1",
    "express-openapi-validator": "^4.12.15",
    "helmet": "^4.6.0",
    "http-errors": "^1.7.3",
    "http-terminator": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "minimatch": "^3.0.4",
    "mongoose": "^5.10.1",
    "mongoose-fuzzy-searching": "^2.0.2",
    "morgan": "~1.9.1",
    "node-pty": "^0.9.0",
    "remove": "^0.1.5",
    "socket.io": "^2.4.1",
    "swagger-jsdoc": "^6.1.0",
    "tar-stream": "^2.1.4",
    "tmp": "^0.2.1",
    "winston": "^3.3.3",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/async": "^3.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.5",
    "@types/debug": "^4.1.5",
    "@types/docker-modem": "^3.0.1",
    "@types/dockerode": "^3.2.1",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^8.0.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^13.11.1",
    "@types/sinon": "^9.0.10",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.35",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/tar-stream": "^2.1.0",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.14.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "choma": "^1.2.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "nodemon": "^2.0.3",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "socket.io-client": "^2.3.0",
    "supertest": "^6.0.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "typescript-eslint-parser": "^22.0.0"
  }
}
